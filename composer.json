{
    "name": "EnjoyYourBusiness/websocketserverbundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Mechanics for a php websocket server using ratchet",
    "authors": [
        {
            "name": "Enjoy Your Business",
            "email": "dev@enjoyyourbusiness.fr",
            "homepage": "https://www.enjoyyourbusiness.fr/"
        }
    ],
    "autoload": {
        "psr-4": { "EnjoyYourBusiness\\WebSocketServerBundle\\": "" }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:EnjoyYourBusiness/WebSocketClientBundle.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:Thuata/ListenerBundle.git"
        }
    ],
    "require": {
        "php": "~7.1",
        "symfony/symfony": "~3",
        "incenteev/composer-parameter-handler": "~2.0",
        "cboden/ratchet": "^0.3",
        "EnjoyYourBusiness/websocketclientbundle": "dev-master",
        "thuata/listenerbundle": "dev-master"
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "master"
        }
    }
}
